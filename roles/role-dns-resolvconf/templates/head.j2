#     DO NOT EDIT THIS FILE BY HAND -- YOUR CHANGES WILL BE OVERWRITTEN
# Ansible start
{% if dns_domain is defined %}
domain {{dns_domain}}
{% endif %}
{% for name in dns_searchs %}
search {{name}} 
{% endfor %}
{% for nameserver in dns_nameservers %}
nameserver {{nameserver}}
{% endfor %}
# Ansible end
